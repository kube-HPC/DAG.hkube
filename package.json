{
  "name": "@hkube/dag",
  "version": "2.1.8",
  "description": "handle the pipeline flow",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --colors --recursive --exit",
    "test:cov": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js --exclude=bootstrap.js --reporter=html --reporter=text --reporter=lcov mocha tests --colors --recursive --exit",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "npm publish"
  },
  "keywords": [
    "hkube"
  ],
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "author": "Hkube",
  "license": "MIT",
  "dependencies": {
    "@hkube/consts": "^1.0.29",
    "@hkube/parsers": "^2.1.5",
    "@hkube/redis-utils": "^1.0.3",
    "@hkube/uid": "^1.0.4",
    "graphlib": "^2.1.8",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "clone": "^2.1.2",
    "coveralls": "^3.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^6.0.2",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2"
  }
}
